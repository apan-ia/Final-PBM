<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="tampilanDaftar1RowModel" type="com.application.app.modules.tampilandaftar.data.model.TampilanDaftar1RowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearPasswordsec"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <TextView android:alpha="0.7" android:ellipsize="end"
            android:id="@+id/txtPassword"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{tampilanDaftar1RowModel.txtPassword}"
            style="@style/txtRalewaysemibold15_4" tools:text="@string/lbl_password"/>
        <LinearLayout android:id="@+id/linearGroup310"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_200cornerRadius">
            <LinearLayout android:id="@+id/linearGroup322"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.85" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_vector_8"/>
                <TextView android:alpha="0.7" android:ellipsize="end"
                    android:id="@+id/txt"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{tampilanDaftar1RowModel.txt}"
                    style="@style/txtRalewaybold15" tools:text="@string/lbl"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageGroup9"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_186pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY" android:src="@drawable/img_group9"/>
        </LinearLayout>
    </LinearLayout>
</layout>
