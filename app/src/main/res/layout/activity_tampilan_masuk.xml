<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="tampilanMasukVM" type="com.application.app.modules.tampilanmasuk.data.viewmodel.TampilanMasukVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearTampilanMasuk"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearBarAtas"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_249pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearBarAtas1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegreen_300">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup324"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="@dimen/_34pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_63pxh"
                            android:layout_width="@dimen/_72pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_vector"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtReDoc"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_36pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_weight="0.76"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{tampilanMasukVM.tampilanMasukModel.txtReDoc}"
                            style="@style/txtRalewayromansemibold30" tools:text="@string/lbl_re_doc"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtSilakanLoginA"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent"
                android:text="@{tampilanMasukVM.tampilanMasukModel.txtSilakanLoginA}"
                style="@style/txtRalewayromansemibold25" tools:text="@string/msg_silakan_login_a"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignup"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearLogin"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearUsername"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:alpha="0.7"
                            android:ellipsize="end"
                            android:id="@+id/txtUsername"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{tampilanMasukVM.tampilanMasukModel.txtUsername}"
                            style="@style/txtRalewaysemibold15_4" tools:text="@string/lbl_username"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_200_radius_10"
                            android:drawablePadding="@dimen/_10pxh"
                            android:drawableStart="@drawable/img_icon_1"
                            android:ellipsize="end"
                            android:hint="@string/lbl_username"
                            android:id="@+id/etUsername"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_16pxv"
                            android:paddingStart="@dimen/_11pxh"
                            android:paddingTop="@dimen/_16pxv"
                            android:singleLine="true"
                            android:text="@={tampilanMasukVM.tampilanMasukModel.etUsernameValue}"
                            android:textColorHint="@color/bluegray_400_99" style="@style/etSolidRounded_3"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:alpha="0.7"
                            android:ellipsize="end"
                            android:id="@+id/txtPassword"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{tampilanMasukVM.tampilanMasukModel.txtPassword}"
                            style="@style/txtRalewaysemibold15_4" tools:text="@string/lbl_password"/>
                        <LinearLayout android:id="@+id/linearGroup325"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_200cornerRadius">
                            <LinearLayout
                                android:id="@+id/linearGroup327"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_9pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_weight="0.86"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageVector1"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY" android:src="@drawable/img_vector_10"/>
                                <TextView android:alpha="0.7"
                                    android:ellipsize="end"
                                    android:id="@+id/txt"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_weight="0.83"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{tampilanMasukVM.tampilanMasukModel.txt}"
                                    style="@style/txtRalewaybold15_1" tools:text="@string/lbl"/>
                            </LinearLayout>
                            <ImageView android:id="@+id/imageGroup9"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginEnd="@dimen/_25pxh"
                                android:layout_marginStart="@dimen/_184pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:layout_width="@dimen/_15pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_group9_2"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearForgotsection"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatCheckBox
                        android:buttonTint="@color/bluegray_400_99"
                        android:ellipsize="end"
                        android:fontFamily="@font/raleway"
                        android:id="@+id/checkBoxGroup47"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.58"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/msg_ingatkan_akun_s"
                        android:textColor="@color/bluegray_400_99"
                        android:textFontWeight="500"
                        android:textSize="@dimen/_12pxh"
                        android:textStyle="bold" tools:text="@string/msg_ingatkan_akun_s"/>
                    <TextView android:alpha="0.7"
                        android:ellipsize="end"
                        android:id="@+id/txtLupaPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_83pxh"
                        android:layout_weight="0.42"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{tampilanMasukVM.tampilanMasukModel.txtLupaPassword}"
                        style="@style/txtRalewaymedium12_2" tools:text="@string/lbl_lupa_password"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:clickable="true" android:ellipsize="end"
                    android:gravity="center" android:id="@+id/btnMasuk"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_masuk"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_masuk"/>
            </LinearLayout>
            <LinearLayout android:clickable="true"
                android:gravity="center_vertical"
                android:id="@+id/linearBottomsec"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBelumPunyaAku"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.76"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{tampilanMasukVM.tampilanMasukModel.txtBelumPunyaAku}"
                    style="@style/txtRalewaysemibold15_5" tools:text="@string/msg_belum_punya_aku"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDaftar"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{tampilanMasukVM.tampilanMasukModel.txtDaftar}"
                    style="@style/txtRalewaysemibold15_5" tools:text="@string/lbl_daftar"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
