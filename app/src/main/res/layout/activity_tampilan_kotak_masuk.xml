                        android:layout_marginStart="@dimen/_48pxh"
                        android:layout_weight="0.24"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{tampilanKotakMasukVM.tampilanKotakMasukModel.txtReservasi1}"
